<%= render 'info', user: current_user, book: @book %>

<h1>Users</h1>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= image_tag user.get_profile_image %></td>
      <td><%= user.name %></td>
      <td><%= link_to "show", "/users/#{user.id}" %></td>
    </tr>
    <% end %>
  </tbody>
</table>
